to   = as.Date("1962-12-31"),
by   = "day"
)
rain = data.frame(date = dates, value = rain)
set.seed(123)  # for reproducibility
# create daily dates
dates <- seq(
from = as.Date("1920-01-01"),
to   = as.Date("1990-12-31"),
by   = "day"
)
# simulate fake sea level data (in metres)
# mean around 1.5 m, with seasonality and some noise
n <- length(dates)
sea_level <- 1.5 +
0.3 * sin(2 * pi * as.numeric(format(dates, "%j")) / 365) + # seasonal effect
0.001 * (1:n) / 365 +                                       # small trend
rnorm(n, mean = 0, sd = 0.1)                                # random noise
# combine into a data frame
sea_levels <- data.frame(
date = dates,
sea_level_m = round(sea_level, 3)
)
head(sea_levels)
tail(sea_levels)
library(ggplot2)
# 1) Data with years on the horizontal axis
p1 <- ggplot(sea_levels, aes(x = date, y = sea_level_m)) +
geom_line() +
scale_x_date(date_breaks = "5 years", date_labels = "%Y") +
labs(x = "Year", y = "Sea level (m)", title = "Daily sea levels")
p1
# 1) Data with years on the horizontal axis
p1 <- ggplot(sea_levels, aes(x = date, y = sea_level_m)) +
geom_point() +
scale_x_date(date_breaks = "5 years", date_labels = "%Y") +
labs(x = "Year", y = "Sea level (m)", title = "Daily sea levels")
p1
# 2) As (1), plus vertical dashed red lines for every year
year_ticks <- seq(as.Date("1920-01-01"), as.Date("1990-01-01"), by = "1 year")
p2 <- ggplot(sea_levels, aes(x = date, y = sea_level_m)) +
geom_point() +
geom_vline(xintercept = year_ticks, linetype = "dashed", color = "red") +
scale_x_date(date_breaks = "5 years", date_labels = "%Y") +
labs(x = "Year", y = "Sea level (m)", title = "Daily sea levels with yearly guides")
p2
