# 4) Add a horizontal red dashed line at the 80% quantile
q80 <- quantile(sea_levels_quarterly$sea_level_m, 0.80, na.rm = TRUE)
p4 <- ggplot(sea_levels_quarterly, aes(x = date, y = sea_level_m)) +
geom_point() +
geom_hline(yintercept = q80, linetype = "dashed", color = "red") +
scale_x_date(
date_breaks = "10 years",
date_labels = "%Y",
limits = as.Date(c("1920-01-01", "1990-12-31"))
) +
labs(x = "Year", y = "Sea level (m)", title = "Sea levels")
p4 + theme_minimal(base_size = 16)
ggsave("/Users/danielacastrocamilo/Dropbox/Glasgow/Supervising/PhDStudents/KirstyRyman/RotationProjectPresentation/p4.pdf", width = 9, height = 6)
